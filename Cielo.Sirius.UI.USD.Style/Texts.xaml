    <Style x:Key="ErrorTextDetail" TargetType="TextBlock" BasedOn="{StaticResource BaseText}">
        <Setter Property="Foreground" Value="{DynamicResource ErrorColor}"/>
        <Setter Property="FontWeight" Value="SemiBold"/>
