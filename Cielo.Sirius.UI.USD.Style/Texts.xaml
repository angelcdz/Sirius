        <Setter Property="FontSize" Value="12"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="Padding" Value="0"/>
    </Style>

    <Style x:Key="ErrorLabelTextDetail" TargetType="Label">
