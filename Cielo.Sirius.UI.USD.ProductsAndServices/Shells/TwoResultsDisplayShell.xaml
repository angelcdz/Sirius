<UserControl x:Class="Cielo.Sirius.UI.USD.ProductsAndServices.Shells.TwoResultsDisplayShell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:nav="clr-namespace:Cielo.Sirius.Foundation.USD.Navegation;assembly=Cielo.Sirius.Foundation.USD"
             Height="469" Width="1001">
    <Grid>
        <ContentControl x:Name="FirstResultDisplayRegion" Width="1001" Height="315" VerticalAlignment="Top"
                        nav:NavigationRegion.RegionName="FirstResultDisplayRegion"/>
        <ContentControl x:Name="FirstResultsListRegion" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="186,38,0,0" 
                        MinWidth="815" MinHeight="277" FocusVisualStyle="{x:Null}"
                        nav:NavigationRegion.RegionName="FirstResultsListRegion"/>

        <ContentControl x:Name="SecondResultDisplayRegion" Height="149" Width="1001" VerticalAlignment="Bottom"
                        nav:NavigationRegion.RegionName="SecondResultDisplayRegion"/>
        <ContentControl x:Name="SecoundResultsListRegion" HorizontalAlignment="Right"  VerticalAlignment="Bottom"
                        MinWidth="815" MinHeight="125" FocusVisualStyle="{x:Null}"
                        nav:NavigationRegion.RegionName="SecoundResultsListRegion"/>
    </Grid>
</UserControl>
